<config>
    <modules>
        <Driskell_CookieCutter>
             <version>1.0.4</version>
        </Driskell_CookieCutter>
    </modules>
    <global>
        <models>
            <driskell_cookiecutter>
                <class>Driskell_CookieCutter_Model</class>
            </driskell_cookiecutter>
        </models>
        <events>
            <controller_front_init_before>
                <observers>
                    <driskell_cookiecutter>
                        <class>driskell_cookiecutter/observer</class>
                        <method>triggerCookieCutter</method>
                    </driskell_cookiecutter>
                </observers>
            </controller_front_init_before>
        </events>
    </global>
    <default>
        <driskell_cookiecutter>
            <general>
                <enabled>0</enabled>
            </general>
        </driskell_cookiecutter>
    </default>
</config>
